          },
          "rating": {
            "type": "number",
            "format": "double"
          },
          "genre": {
            "type": "string"
          }
        },
        "required": [
          "director",
          "genre",
          "imdbId",
          "synopsis",
          "title",
          "year"
        ]
      },
      "FilmResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "title": {
            "type": "string"
          },
          "director": {
            "type": "string"
          },
          "imdbId": {
            "type": "string"
          },
          "synopsis": {
            "type": "string"
          },
          "filmPoster": {
            "type": "string",
            "format": "byte"
          },
          "year": {
            "type": "string"
          },
          "archive": {
            "type": "boolean"
          },
          "rating": {
            "type": "number",
            "format": "double"
          },
          "genre": {
            "type": "string",
            "enum": [
              "ACTION",
              "ADVENTURE",
              "ANIMATION",
              "BIOGRAPHY",
              "COMEDY",
              "CRIME",
              "DOCUMENTARY",
              "DRAMA",
              "FAMILY",
              "FANTASY",
              "HISTORY",
              "HORROR",
              "MUSICAL",
              "MYSTERY",
              "ROMANCE",
              "SCI_FI",
              "SPORT",
              "THRILLER",
              "WAR",
              "WESTERN"
            ]
          },
          "addedBy": {
            "type": "integer",
            "format": "int32"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "lastModifiedAt": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "FilmRentalHistoryRequest": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "filmId": {
            "type": "integer",
            "format": "int32"
          },
          "rentalDate": {
            "type": "string",
            "format": "date-time"
          },
          "returnDate": {
            "type": "string",
            "format": "date-time"
          },
          "returned": {
            "type": "boolean"
          },
          "returnedApproved": {
            "type": "boolean"
          },
          "rentalPrice": {
            "type": "number",
            "format": "double"
          }
        },
        "required": [
          "filmId",
          "rentalDate",
          "userId"
        ]
      },
      "FilmRentalHistoryResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "rentalDate": {
            "type": "string",
            "format": "date-time"
          },
          "returnDate": {
            "type": "string",
            "format": "date-time"
          },
          "returned": {
            "type": "boolean"
          },
          "returnedApproved": {
            "type": "boolean"
          },
          "rentalPrice": {
            "type": "number",
            "format": "double"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "filmId": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "FilmFeedbackRequest": {
        "type": "object",
        "properties": {
          "filmId": {
            "type": "integer",
            "format": "int32"
          },
          "rating": {
            "type": "number",
            "format": "double",
            "maximum": 5,
